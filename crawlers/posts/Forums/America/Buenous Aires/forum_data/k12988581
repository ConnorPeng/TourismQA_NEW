$("#" + placement.id);\n// since the shelf items are mixed type, it\'s difficult to use a fixed height, this function\n// find out the tallest shelf item and adjust containers accordingly\nfunction setUpContainer() {\nvar maxHeight = 0;\nplacementEl.find(\'.shelf_container.forum_mention .scrollable_container .ui_column\').each(function() {\nvar height = $(this).outerHeight();\nif (height > maxHeight) {\nmaxHeight = height;\n}\n});\nif (maxHeight > 0)\n{\nplacementEl.find(\'.shelf_container.forum_mention\').height(maxHeight + 24);\nplacementEl.find(\'.forum_mention .shelf_item_container\').height(maxHeight);\nplacementEl.find(\'.forum_mention .scrollable_container\').height(maxHeight);\n}\n}\n// if there are more than 3 items on the shelf, we need to set up handlers for the left/right scrolls.\nfunction setUpScroller() {\nvar scroller = placementEl.find(\'.scrollable_container\');\nvar leftArrow = placementEl.find(\'.arrow.left\');\nvar rightArrow = placementEl.find(\'.arrow.right\');\nvar itemOffset = scroller.find(\'.ui_column\').outerWidth();\n// number of items on the shelf\nvar itemCount = parseInt(scroller.data(\'numitems\'));\n// right most offset (left most is 0)\nvar scrollLimit = 3 - itemCount;\nvar offset = 0;\nfunction showArrows() {\nvar leftCss = \'\';\nvar rightCss = \'\';\n// check left arrow\nif (offset < 0) {\nleftCss = \'block\';\n}\n// check right arrow\nif (offset > scrollLimit) {\nrightCss = \'block\';\n}\nleftArrow.css(\'display\', leftCss);\nrightArrow.css(\'display\', rightCss);\n}\nfunction scroll(offset)\n{\nscroller.animate({\nleft: offset * itemOffset\n}, 500, function() {});\nshowArrows();\n}\nfunction scrollLeft() {\nif (offset < 0)\n{\n// only scroll 3 items at a time\nif (offset < -3) {\noffset += 3;\n}\nelse {\noffset = 0;\n}\nscroll(offset);\n}\n}\nfunction scrollRight() {\nvar scrollLimit = 3 - itemCount;\nif (offset > scrollLimit) {\noffset -= 3;\nif (offset < scrollLimit) {\noffset = scrollLimit;\n}\nscroll(offset);\n}\n}\nshowAr